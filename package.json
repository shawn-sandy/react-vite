        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/react-test-renderer": "^18.0.0",
        "@vitejs/plugin-react": "^1.3.0",
        "@vitest/ui": "^0.15.1",
        "c8": "^7.11.3",
        "happy-dom": "^5.3.1",
        "jsdom": "^19.0.0",
        "react-test-renderer": "^18.2.0",
        "typescript": "^4.6.3",
        "vite": "^2.9.9",
        "vitest": "^0.15.1"
    },
    "dtpublish": {
